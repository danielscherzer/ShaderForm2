<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <Version>0.1.0</Version>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <RootNamespace>ShaderForm2</RootNamespace>
    <Company>Daniel Scherzer</Company>
    <Authors>Daniel Scherzer</Authors>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <Copyright>2021 Daniel Scherzer</Copyright>
    <ApplicationIcon>content\shaderform2.ico</ApplicationIcon>
  </PropertyGroup>
  
  <ItemGroup>
    <Resource Include="content\shaderform2.ico" />
    <EmbeddedResource Include="content\checker.frag" />
    <EmbeddedResource Include="content\screenQuad.vert" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="AutoUpdateViaGitHubRelease" Version="0.1.53" />
    <PackageReference Include="GLSLhelper" Version="0.3.52" />
    <PackageReference Include="HandyControls" Version="3.3.9" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="OpenTK" Version="4.6.7">
      <NoWarn>NU1608</NoWarn>
    </PackageReference>
    <PackageReference Include="OpenTK.GLWpfControl" Version="4.1.0" />
    <PackageReference Include="ReactiveUI.WPF" Version="16.4.1" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
    <PackageReference Include="Zenseless.OpenTK" Version="0.1.36" />
    <PackageReference Include="Zenseless.Patterns" Version="0.6.18" />
    <PackageReference Include="Zenseless.Resources" Version="0.1.9" />
  </ItemGroup>
  
</Project>