<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <Version>0.1.0</Version>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows10.0.19041</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <RootNamespace>ShaderForm2</RootNamespace>
    <Company>Daniel Scherzer</Company>
    <Authors>Daniel Scherzer</Authors>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <Copyright>2021 Daniel Scherzer</Copyright>
    <ApplicationIcon>content\shaderform2.ico</ApplicationIcon>
  </PropertyGroup>
  
  <ItemGroup>
    <Resource Include="content\shaderform2.ico" />
    <EmbeddedResource Include="content\checker.frag" />
    <EmbeddedResource Include="content\screenQuad.vert" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="AutoUpdateViaGitHubRelease" Version="0.1.63" />
    <PackageReference Include="GLSLhelper" Version="0.3.61" />
    <PackageReference Include="HandyControls" Version="3.4.5" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="OpenTK.GLWpfControl" Version="4.2.3" />
    <PackageReference Include="System.Reactive" Version="6.0.0" />
    <PackageReference Include="Zenseless.OpenTK" Version="0.1.74" />
    <PackageReference Include="Zenseless.PersistentSettings" Version="0.1.8" />
  </ItemGroup>
  
</Project>